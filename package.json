{
  "name": "design-system-poc",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && build-storybook",
    "start": "next start",
    "lint": "next lint",
    "storybook": "start-storybook -p 6006 -s ./public",
    "token-transformer": "token-transformer data/tokens.json data/output.json youwe --expandTypography=true",
